{
  "name": "next-netlify-starter",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:mock\" \"next dev\"",
    "build": "jest --forceExit && next build",
    "export": "next export",
    "start": "next start",
    "dev:mock": "json-server --watch .json-server/db.json --port 3004",
    "tailwind:watch": "tailwindcss -i ./styles/globals.css -o ./dist/output.css --watch",
    "lint": "next lint --dir .",
    "test": "jest --watch"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "cypress": "^10.0.3",
    "isomorphic-fetch": "^3.0.0",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "18.0.24",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "json-server": "^0.17.0",
    "msw": "^0.47.4",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4"
  }
}
